    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.fontSize": 12,
    "editor.mouseWheelScrollSensitivity": 3,
    "editor.minimap.enabled": false,

    "[typescript]": {
